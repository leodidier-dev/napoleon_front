{
    "semi": true,
    "useTabs": false,
    "tabWidth": 2,
    "printWidth": 140,
    "trailingComma": "all",
    "singleQuote": true,
    "endOfLine": "lf",
    "importOrder": [
        "^~/components(.*)$",
        "^~/composables(.*)$",
        "^~/assets(.*)$",
        "^~/scss(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
